@CHARSET "UTF-8";
 .black_overlay{ 
            display: none; 
            position: absolute; 
            top: 0%; 
            left: 0%; 
            width: 100%; 
            height: 190%; 
            background-color:#000000;
            z-index:1001; 
            -moz-opacity: 0.8; 
            opacity:.80; 
            filter: alpha(opacity=80); 
        } 
        .window { 
            display: none; 
            position: fixed; 
            top: 20%;
            left: 35%; 
            width: 30%; 
            height: 40%; 
            padding: 0px; 
            background-color: white; 
            border: 10px solid #eeeee; 
            z-index:1002; 
            overflow: auto; 
        } 
        .window_title{
			background: rgb(204, 204, 244); padding: 0px 24px; height: 40px; line-height: 40px; position: relative;
		}
        .window_title h2 {
			font: bold 16px/40px "微软雅黑"; color: rgb(2, 105, 102); font-size-adjust: none; font-stretch: normal;
		}
		.window_close {
			background: url("../images/t01d4ca4cea4b107823.png") no-repeat; _background: url(../images/t01614f54e4c2610bc6.png) no-repeat;
			background-position: -100px -100px; top: 10px; width: 20px; height: 20px; right: 10px; position: fixed; cursor: pointer;
		}
		.window_content table {
			width: 300px; height: 170px; border: rgb(223, 223, 223) 1px solid; margin-top: 20px; margin-left: 25px;	font-family: 微软雅黑; font-size: 15px;
		}
		.window_content  th {
			background: rgb(234, 234, 234); height: 27px; color: rgb(102, 102, 102); font-weight: bold;
		}
		.window_content td {
			height: 29px;  color: rgb(51, 51, 51); 
		}