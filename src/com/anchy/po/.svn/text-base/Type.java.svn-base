package com.anchy.po;

import java.io.Serializable;

public class Type implements Serializable {
	private int tid;
	private Goods goods;
	private String kind;
	private int number;
	private String photo;
	public Type() {
		super();
	}
	public Type(int tid, Goods goods, String kind, int number, String photo) {
		super();
		this.tid = tid;
		this.goods = goods;
		this.kind = kind;
		this.number = number;
		this.photo = photo;
	}
	public int getTid() {
		return tid;
	}
	public void setTid(int tid) {
		this.tid = tid;
	}
	public Goods getGoods() {
		return goods;
	}
	public void setGoods(Goods goods) {
		this.goods = goods;
	}
	public String getKind() {
		return kind;
	}
	public void setKind(String kind) {
		this.kind = kind;
	}
	public int getNumber() {
		return number;
	}
	public void setNumber(int number) {
		this.number = number;
	}
	public String getPhoto() {
		return photo;
	}
	public void setPhoto(String photo) {
		this.photo = photo;
	}
	
	
}
